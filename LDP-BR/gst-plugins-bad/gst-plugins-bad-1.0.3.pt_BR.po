#: ext/resindvd/resindvdsrc.c:359
msgid "Could not read title information for DVD."
msgstr "Não foi possível ler as informações de título do DVD."

#: ext/resindvd/resindvdsrc.c:365
#, c-format
msgid "Failed to open DVD device '%s'."
msgstr "Falha ao abrir o dispositivo de DVD \"%s\"."

#: ext/resindvd/resindvdsrc.c:371
msgid "Failed to set PGC based seeking."
msgstr "Falha ao definir busca baseada em PGC."

#: ext/resindvd/resindvdsrc.c:1156
msgid ""
"Could not read DVD. This may be because the DVD is encrypted and a DVD "
"decryption library is not installed."
msgstr ""
"Não foi possível ler o DVD. O motivo pode ser que o DVD está criptografado "
"ou uma biblioteca de descriptografia não está instalada."

#: ext/resindvd/resindvdsrc.c:1161 ext/resindvd/resindvdsrc.c:1170
msgid "Could not read DVD."
msgstr "Não foi possível ler o DVD."

#: ext/sndfile/gstsfsink.c:277 ext/sndfile/gstsfsrc.c:346
msgid "No file name specified for writing."
msgstr "Nenhum nome de arquivo especificado para gravação."

#: ext/sndfile/gstsfsink.c:290 ext/sndfile/gstsfsrc.c:352
#, c-format
msgid "Could not open file \"%s\" for writing."
msgstr "Não foi possível abrir o arquivo \"%s\" para gravação."

#: ext/sndfile/gstsfsink.c:439 gst/nuvdemux/gstnuvdemux.c:737
msgid "Internal data stream error."
msgstr "Erro interno de fluxo de dados."

#: ext/sndfile/gstsfsink.c:493 ext/sndfile/gstsfsink.c:501
#, c-format
msgid "Could not write to file \"%s\"."
msgstr "Não foi possível gravar no arquivo \"%s\"."

#: gst/aiff/aiffparse.c:1326
msgid "Internal data flow error."
msgstr "Erro interno de fluxo de dados."

#: sys/dvb/gstdvbsrc.c:758 sys/dvb/gstdvbsrc.c:855
#, c-format
msgid "Device \"%s\" does not exist."
msgstr "O dispositivo \"%s\" não existe."

#: sys/dvb/gstdvbsrc.c:762
#, c-format
msgid "Could not open frontend device \"%s\"."
msgstr "Não foi possível abrir o dispositivo de interface \"%s\"."

#: sys/dvb/gstdvbsrc.c:776
#, c-format
msgid "Could not get settings from frontend device \"%s\"."
msgstr ""
"Não foi possível obter as configurações do dispositivo de interface \"%s\"."

#: sys/dvb/gstdvbsrc.c:859
#, c-format
msgid "Could not open file \"%s\" for reading."
msgstr "Não foi possível abrir o arquivo \"%s\" para leitura."
